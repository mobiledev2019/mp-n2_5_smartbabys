<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="#2a3c4d"/>
            <corners android:radius="@dimen/x50dp"/>
            <stroke android:width="@dimen/x5dp" android:color="#c9c9c9"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="#91a85e"/>
            <corners android:radius="@dimen/x50dp"/>
        </shape>
    </item>
</selector>
